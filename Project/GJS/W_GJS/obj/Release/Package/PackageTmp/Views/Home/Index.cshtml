    <script>
        $("#topMain li a").click(function () {
            $('li').removeClass('active');
            $(this).parent().addClass('active');
        });

        $("#topMain .dropdown-menu li a").click(function () {
            $('li').removeClass('active');
            $(this).parent().parent().parent().addClass('active');
        });


        function showPopup() {
            var needPopup = @(Session["needPopup"] == null ? "true" : "false");
            @{
                Session["needPopup"] = true;
            }
            if (needPopup) {
                $("#PopupModal").modal();
            }
        }
        showPopup();
